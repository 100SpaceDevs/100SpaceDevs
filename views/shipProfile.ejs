<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/reset.css" />
    <link rel="stylesheet" href="css/hero.css" />
    <title>User Profile</title>
  </head>
  <body class="container">
    <section class="title">
      <h1>100 Space Devs</h1>
    </section>
    <form action="/" method="POST" class="form__container">
      <input type="text" name="name" placeholder="Name Your Ship" />
      <button type="submit"><span>Add Name</span></button>
    </form>
    <!-- <section class="image__container"></section> -->
    <section class="hero">
      <section id="launchInfo" class="info__container">
        <img class="launchPhoto" src="" alt="Falcon 9 Rocket" />
      </section>
      <div class="carousel-container">
        <!-- Full-width images with number and caption text -->
        <div class="mySlides fade">
          <h1>Ship01 Name</h1>
          <div class="ship-container">
            <img src="../public/img/Falcon 1.png" alt="falcon1" />
          </div>
        </div>

        <div class="mySlides fade">
          <h1>Ship02 Name</h1>
          <div class="ship-container">
            <img src="../public/img/Falcon 1.png" alt="falcon1" />
          </div>
        </div>

        <div class="mySlides fade">
          <h1>Ship03 Name</h1>
          <div class="ship-container">
            <img src="../public/img/Falcon 1.png" alt="falcon1" />
          </div>
        </div>

        <!-- Next and previous buttons -->
        <a class="prev" data-arrow="190" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" data-arrow="188" onclick="plusSlides(1)">&#10095;</a>
      </div>
      <section id="rocketInfo" class="info__container"></section>
    </section>
